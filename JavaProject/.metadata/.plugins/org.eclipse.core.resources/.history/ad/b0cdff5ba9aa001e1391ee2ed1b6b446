package day0104;

public class QuizDataType_10 {

	public static void main(String[] args) {
		// 변수는 args를 이용할 것
		
		/*
		 * 학생명 : 김선호 님
		 * 지역 : 부산
		 * 나이 : 23세
		 * 
		 * ==================
		 * 김선호님의 자바점수는 88점, 오라클점수는 77점, 총 165점입니다.
		 */
		
		String name = args[0];
		String local = args[1];
		int age = (int)args[2];
		int java = args[3];
		int oracle = args[4];
		
		
		System.out.println();

	}

}
