package day0108;

public class L07_For {

	public static void main(String[] args) {
		// while문과 for문을 비교
		
		System.out.println("**while문으로 10~0출력**");
		int i=10;
		while(i >= 0) {
			System.out.print(i-- +" ");
		}
		System.out.println();
		
		System.out.println("**for문으로 10~0출력**");
		for(int a=10; a>=0; a--) {
			System.out.print(a+ " ");
		}
		System.out.println();
		
		System.out.println("**for문 1~10 종료**");
		for(int a=1; a<=10; a++) {
			System.out.println(a+" ");
		}

	}

}
