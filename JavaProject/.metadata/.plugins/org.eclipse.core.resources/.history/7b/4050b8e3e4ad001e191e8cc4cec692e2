package day0108;

public class L12_QuizFor {

	public static void main(String[] args) {
		
		System.out.println("#1. 1  2  4  5  7  8  10 (continue 활용할 것!!");
		for(int i=1; i<=10; i++) {
			if(i%3==0) {
				continue;
			}
			System.out.print(i + " ");
		}
		System.out.println();
		
		
		
		System.out.println("#2. ");

	}

}
