package day0108;

public class L09_For {

	public static void main(String[] args) {
		
		// 가로로 1~10까지 출력
		for(int i=1; i<=10; i++) {
			System.out.print(i+" ");
		}
		System.out.println();
		
		// 10~1까지 출력
		for(int i=10; i>=1; i++) {
			System.out.println(i + " ");
		}
		
		
		// 배수, continue

	}

}
