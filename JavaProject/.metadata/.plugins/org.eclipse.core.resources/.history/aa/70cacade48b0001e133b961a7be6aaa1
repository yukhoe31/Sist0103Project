package day0111;

class MyInfo{
	
	private String name;
	private String mbti;
	private int age;
	
	//생성자
	public MyInfo(String name, String mbti, int age) {
		this.name = name;
		this.mbti = mbti;
		this.age = age;
	}
	
	//메서드
	public void writeTitle() {
		System.out.println("이름\tMBTI\t나이");
		System.out.println("========================");
	}
	
}

