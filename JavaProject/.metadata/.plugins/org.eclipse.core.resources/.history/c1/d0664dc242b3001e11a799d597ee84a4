}
////////////////
class Modify implements Command{

	@Override
	public void write() {
		// TODO Auto-generated method stub
		System.out.println("게시판에 게시물을 수정합니다.");
	}
	
}
////////////////
class Delete implements Command{

	@Override
	public void write() {
		// TODO Auto-generated method stub
		System.out.println("게시판에 게시물을 삭제합니다.");
	}
	
}
