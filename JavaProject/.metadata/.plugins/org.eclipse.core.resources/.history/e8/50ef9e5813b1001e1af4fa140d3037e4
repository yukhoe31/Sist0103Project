package day0112;

//부모
class Work{
	public void process() {
		System.out.println("아무것도 안하고 있음!!!");
	}
	
}
//////////////////////////자식들
class Food1 extends Work{
	
	@Override
	public void process() {
		// TODO Auto-generated method stub
		//super.process();
		System.out.println("***음식담당***");
		
	}
}
///////////////////////////
class Clean extends Work{
	
	@Override
	public void process() {
		// TODO Auto-generated method stub
		//super.process();
		System.out.println("***청소담당***");
		
	}
}

