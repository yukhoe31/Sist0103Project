package day0108;

public class L10_For {

	public static void main(String[] args) {

		int i=1;
		
		for(; i<=10; i++) {
			System.out.print(i+" ");
		}
		
		System.out.println("\nfor문 빠져나온 i의 값: " + i);
		
		int a = 10;
		for(; a<=50; a+=5) {
			System.out.println(a + " ");
		}
		System.out.println("\n빠져나온 a값: " + a);
	}

}
