package day0110;


class Mart{
	private String sangpum;
	int su;
	int dan;
	static String SHOPNAME = "롯데마트";
	
	
	public Mart() {
		
	}
	
	public Mart(String name, int num, int price) {
		this.sangpum = name;
		this.su = num;
		this.dan = price;
	}
	
	//setter
	public void setName(String name) {
		this.sangpum = name;
	}
	public void setNum(int num) {
		this.su = num;
	}
	public void setPrice(int price) {
		this.dan = price;
	}
	
	//getter
	public String getName() {
		return sangpum;
	}
	public int getNum() {
		return su;
	}
	public int getPrice() {
		return dan;
	}
	
	
}

public class L16_QuizClass {
		
	//생성후 초기값 준 후 출력하시오.
	
	//**롯데마트 입고물풒**
	//======================
	//상품명: 초코파이
	//수량: 10개
	//가격: 4500원
	//----------------------
	//상품명: 엄마손파이
	//수량: 5개
	//가격: 2500원
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Mart m1 = new Mart();
		m1.setName("초코파이");
		m1.setNum(10);
		m1.setPrice(450);//450*10=4500
		
		String name1 = m1.getName();
		int num1 = m1.getNum();
		int price1 = m1.getPrice();
		System.out.println("상품명" + );

	}

}
