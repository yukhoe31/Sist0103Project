package day0105;

public class SwitchTest_17 {

	public static void main(String[] args) {

		//semicolon이 아닌 colon임을 기억하자.

		//switch문 예제_#1
		int a =3;
		
		//break가 없을경우 끝까지 실행
		switch(a) {
			case 1:
				System.out.println("a is 1");
			case 2:
				System.out.println("a is 2");
			case 3:
				System.out.println("a is 3");
			case 4:
				System.out.println("a is 4");
		}

	}

}
