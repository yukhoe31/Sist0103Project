package day0104;

import java.util.Scanner;

public class book70_hw_2 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		System.out.print("x값 입력: ");
		int x = sc.nextInt();
		
		System.out.print("y값 입력: ");
		int y = sc.nextInt();
		
		int result = x+y;
		
		System.out.println("x + y = " + result);
			
		
	}

}
