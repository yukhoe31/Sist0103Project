package day0104;

public class DataType_06 {

	//boolean flag; <-(메서드 밖에서)이렇게 주면 false로 인식
	public static void main(String[] args) {
		// 기본 자료형 8개
		
		// 1.논리형: true, false
		boolean flag = false;//<-(메서드안에서)줄때는 '대입'부분이 없으면 오류남.
		
		// 2.문자형
		//반드시 하나의 문자, 16바이트 유니코드값 표현
		//2바이트 문자하나
		//기본적으로 정수형, 아스키코드로 변경이 가능
		//''로 표현
		char ch = '가';
		
		// 3.정수형
		byte a = 128;
		
		
	}

}
