package day0111;

class MyCar1{
	String carName;
	int carPrice;
	String carColor;
}

public class L13_CallByReference {
	
	public static void dataIn(MyCar1 car1) {
		car1.carName = "미니쿠퍼";
		car1.carPrice = 4200000;
		car1.carColor = "진주색";
	}
	
	public static void dataOut() {
		
	}

	public static void main(String[] args) {
		
		

	}

}
