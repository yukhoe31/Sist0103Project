package day0110;


class Mart{
	private String sangpum;
	int su;
	int dan;
	static String SHOPNAME = "롯데마트";
	
	
	public Mart() {
		
	}
	
	public Mart(String name, int num, int price) {
		this.sangpum = name;
		this.su = num;
		this.dan = price;
	}
	
	//setter
	public void setName() {
		this.sangpum = sangpum;
	}
	public void setNum() {
		this.su = su;
	}
	public void setPrice() {
		this.dan = dan;
	}
	
	//getter
}

public class L16_QuizClass {
		
