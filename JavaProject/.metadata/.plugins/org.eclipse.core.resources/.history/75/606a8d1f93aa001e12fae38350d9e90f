package day0103;

public class HelloTest_01 {
	
	//ma + ctrl+스페이스 +엔터:main 메서드 자동 입력
	public static void main(String[] args) {
	
		System.out.print("안녕 자바는 처음이야");
		System.out.println("***main 메서드 매개변수 테스트***");//syso+ctrl+space + 엔터
		System.out.println("1: " + args[0]);
		System.out.println("2: " + args[1]);
		System.out.println("====================");
		System.out.println(args[0] + args[1]);
	
		
	}//ctrl + i : 자동 들여쓰기
	 //ctrl + a : 전체 선택

}
