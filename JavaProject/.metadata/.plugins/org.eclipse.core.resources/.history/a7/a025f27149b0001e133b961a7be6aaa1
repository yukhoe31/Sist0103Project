package day0111;

class MyInfo{
	
	private String name;
	private String mbti;
	private int age;
	
	//생성자
	public MyInfo(String name, String mbti, int age) {
		this.name = name;
		this.mbti = mbti;
		this.age = age;
	}
	
	//메서드
	public void writeTitle() {
		System.out.println("이름\tMBTI\t나이");
		System.out.println("========================");
	}
	public void writeInfo() {
		System.out.println(name+"\t" + mbti + "\t" + age +"세");
	}
	
}

////////////////
public class L12_ArrObTest {

	public static void main(String[] args) {
		
		//생성자 3
		MyInfo [] my1 = new MyInfo[3];
		
		my1[0] = new MyInfo("이효리" ,"ENTP", 26);
		my1[0] = new MyInfo("이상순" ,"ISFJ", 33);
		my1[0] = new MyInfo("이영자" ,"ENFP", 28);

	}

}
