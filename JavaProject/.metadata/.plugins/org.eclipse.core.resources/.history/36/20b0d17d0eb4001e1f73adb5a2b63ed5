package day0116;

class Outer{
	
	String name = "조혜영";
	int age = 22;
	
	//member 내부1	
	class Inner1{
		//내부클래스에서는 외부클래스의 멤버변수사용 가능하다.
		public void disp() {
			System.out.println("**Inner1 내부클래스**");
		}
	}
	//member 내부2	
	class Inner2{
		
	}
	
}

