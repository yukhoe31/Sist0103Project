package day0110;

class MyDate{
	
	private int year;
	private int month;
	private int day;
	
	//디폴트 생성자
	
	//명시적 생성자_ 생성과 동시에 초기화
	public MyDate(int y, int m, int d) {
		
	}
	
	//setter
	public void setYear(int y) {
		this.year = y;
	}
	public void setMear(int m) {
		month = m;
	}
	public void setDear(int d) {
		day = d;
	}
	
	//getter
	public int getYear() {
		return year;
	}
	public int getMonth() {
		return month;
	}
	public int getDate() {
		return day;
	}
	
}



public class L14_MyDate {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
